@article{Remmer2004,
	abstract = {Building on the literature on public finance, I seek to advance our understanding of variations in government size by exploring the impact of official development assistance on fiscal policy. I hypothesize that foreign aid operates in accordance with the ``flypaper effect,'' systematically generating incentives and opportunities for the expansion of government spending. Results from a time-series cross-sectional regression analysis of growth in government spending over the 1970\textendash 99 time period are consistent with the hypothesis. For middle- and lower-income nations, aid represents an important determinant of government expansion. Looking at the tax and revenue side of the equation, however, reveals a more perverse pattern of response: aid promotes not only increased spending but also reduced revenue generation. The results have important implications from both a theoretical and policy perspective. Inter alia they point to the potentially self-defeating nature of efforts to promote market-oriented programs of state retrenchment via development assistance as well as to the importance of incorporating international transfers into future research on government spending. [ABSTRACT FROM AUTHOR]},
	author = {Remmer, Karen L.},
	doi = {10.1111/j.0092-5853.2004.00057.x},
	issn = {0092-5853},
	journal = {American Journal of Political Science},
	keywords = {economic policy,Foreign Aid,government aid,Government Spending,grants in aid (public finance),international economic assistance,international economic relations,international relief,public finance},
	mendeley-tags = {Foreign Aid,Government Spending},
	month = jan,
	number = {1},
	pages = {77--92},
	title = {Does Foreign Aid Promote the Expansion of Government?},
	volume = {48},
	year = {2004},
	bdsk-url-1 = {https://doi.org/10.1111/j.0092-5853.2004.00057.x}}

