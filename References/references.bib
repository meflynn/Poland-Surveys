	publisher = {{SAGE Publications Ltd}},
	shorttitle = {Identifying the Level of Major Power Support Signaled for Prot\'eg\'es},
	title = {Identifying the Level of Major Power Support Signaled for Prot\'eg\'es: {{A}} Latent Measure Approach},
	volume = {56},
