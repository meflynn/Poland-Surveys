@article{KeohaneNye1974,
	abstract = {Students of world politics have tended to assume that states act as units. Yet transgovernmental relations\textendash direct interactions among sub-units not controlled or closely guided by the policies of cabinets or chief executives\textendash are frequently important. Transgovernmental relations are facilitated by extensive personal contacts among officials and by conflicts of interest between departments or agencies within modern governments. International organizations can play important roles in transgovernmental networks by \{(I)\} affecting the definition of issues; (2) promoting coalitions among governmental subunits with similar interests; and (3) serving as points of policy intervention in transnational systems. As policy interdependence among developed-country governments becomes more extensive and complex, these roles of international organizations are likely to become increasingly important. Internationalism of this relatively informal, noninstitutionalized type is not a "dead end."},
	author = {Keohane, Robert O and Nye, Joseph S},
	journal = {World Politics},
	number = {1},
	pages = {39--62},
	title = {Transgovernmental Relations and International Organizations},
	volume = {27},
