	month = oct,
	number = {7},
	pages = {1242--1272},
	publisher = {{SAGE Publications Inc.}},
	title = {Why Bad Governance Leads to Repeat Civil War},
	volume = {59},
	year = {2015},
	bdsk-url-1 = {https://doi.org/10.1177/0022002714528006}}

