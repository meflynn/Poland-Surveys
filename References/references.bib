	title = {Foreign Aid and Political Stability},
	volume = {32},
	year = {2015},
	bdsk-url-1 = {https://doi.org/10.1177/0738894214541227}}

@article{Steinwand2015b,
	abstract = {Donor proliferation and the fragmentation of aid delivery is an important problem besetting foreign aid policy. Increased donor coordination is widely seen as a fix to this problem. This paper explores theoretically and empirically the collective action problems and incentives that donors face when coordinating their actions, based on the distinction between private and public good properties of aid. We introduce the concepts of weak and strong lead donorship, develop a measure that accounts for the exclusive and long-lasting ties between a lead donor and a recipient country, and show that strong lead donorship is in long-term decline. We test our theory combining Spatial Autoregressive (SAR) models, non-parametric model discrimination techniques, and data on aid delivery channels. We recover evidence of collusion in the provision of private good aid in the presence of a strong lead donor, and a lack of coordination and competition in the presence of a weak lead donor. For aid with public good characteristics, we find evidence of coordination in the absence of a lead donor},
	author = {Steinwand, Martin C.},
	doi = {10.1017/S0020818314000381},
	issn = {0020-8183},
	journal = {International Organization},
	month = feb,
	number = {2},
	pages = {443--472},
	publisher = {{Cambridge University Press}},
	title = {Compete or Coordinate? {{Aid}} Fragmentation and Lead Donorship},
	volume = {69},
	year = {2015},
	bdsk-url-1 = {https://doi.org/10.1017/S0020818314000381}}

